<p id="notice"><%= notice %></p>

<h1>Historic Maps. Mainly for providing information for overlay maps</h1>

<%= link_to 'New Map', new_map_path %>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Year</th>
      <th>Notes</th>
      <th>Thomas Guide</th>
      <th>URL</th>
      <th>Attribution Link</th>
      <th>Attribution Text</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @maps.each do |map| %>
      <tr>
        <td><%= map.name %></td>
        <td><%= map.year %></td>
        <td><%= map.notes %></td>
        <td><%= map.thomasGuide%></td>
        <td><%= map.url %></td>
        <td><%= map.attribLink %></td>
        <td><%= map.attrib %></td>
        <td><%= link_to 'Show', map %></td>
        <td><%= link_to 'Edit', edit_map_path(map) %></td>
        <td><%= link_to 'Destroy', map, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Map', new_map_path %>
